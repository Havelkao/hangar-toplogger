{
    "version": 2,
    "builds": [
        {
            "src": "server/main.py",
            "use": "@vercel/python"
        }
    ],
    "installCommand": "pip install -r ../requirements.txt",
    "buildCommand": "gunicorn main:app",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/main.py"
        }
    ]
}
